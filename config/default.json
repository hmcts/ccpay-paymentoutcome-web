{
  "useCSRFProtection": true,
  "security": {
    "referrerPolicy": "origin",
    "clientId": "PAYHUB_CLIENT_ID"
  },
  "session": {
    "cookieName": "SESSION_ID"
  },
  "payhub": {
    "url": "http://localhost:8080"
  },
  "s2s": {
    "url": "http://localhost:8085"
  },
  "appInsights": {
    "instrumentationKey": false
  },
  "e2e": {
    "frontendUrl": "https://localhost:4200/",
    "waitForTimeoutValue": 2600,
    "waitForActionValue": 2000
  },
  "secrets": {
    "ccpay": {
      "paymentoutcome-client-secret": "PAYHUB_CLIENT_SECRET"
    }
  }
}
